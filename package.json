{
	"name": "chatbot",
	"version": "3.1.0",
	"private": true,
	"scripts": {
		"dev": "cross-env BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true next dev --turbo -p 3300",
		"prebuild": "pnpm run lint && pnpm run test:unit",
		"build": "pnpm run prebuild && tsx lib/db/migrate && cross-env BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true next build",
		"start": "next start",
		"lint": "ultracite check",
		"format": "ultracite fix",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "npx tsx lib/db/migrate.ts",
		"db:studio": "drizzle-kit studio",
		"db:push": "drizzle-kit push",
		"db:pull": "drizzle-kit pull",
		"db:check": "drizzle-kit check",
		"db:ping": "tsx scripts/db-ping.ts",
		"db:tables": "tsx scripts/db-tables.ts",
		"db:up": "drizzle-kit up",
		"supabase:link": "npx supabase link",
		"supabase:start": "npx supabase start",
		"supabase:stop": "npx supabase stop",
		"supabase:status": "npx supabase status",
		"supabase:types": "npx supabase gen types typescript --linked > lib/db/supabase-types.ts",
		"supabase:config-push": "npx supabase config push",
		"supabase:api-keys": "npx supabase projects api-keys -o env",
		"supabase:env": "tsx scripts/supabase-env.ts",
		"supabase:setup": "pnpm run supabase:config-push",
		"vercel:link": "npx vercel link",
		"vercel:pull": "npx vercel pull",
		"vercel:env": "npx vercel env pull .env.local",
		"vercel:env:prod": "npx vercel env pull .env.local --environment=production",
		"vercel:deploy": "npx vercel",
		"vercel:deploy:prod": "npx vercel --prod",
		"vercel:open": "npx vercel open",
		"skills:check": "npx skills check",
		"skills:list": "npx skills list",
		"vercel:logs": "npx vercel logs",
		"predeploy": "tsx scripts/pre-deploy.ts",
		"config:check": "tsx scripts/check-config.ts",
		"analyze": "tsx lib/db/migrate && cross-env ANALYZE=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true next build --webpack",
		"test": "export PLAYWRIGHT=True && pnpm exec playwright test",
		"validate:prompt": "tsx lib/prompts/agente-trabalhista/run-validate.ts",
		"test:unit": "pnpm exec vitest run"
	},
